    "icons": {
      "16": "./icons/16.png",
      "32": "./icons/32.png",
      "48": "./icons/48.png",
      "128": "./icons/128.png"
    },
    "permissions": [
      "storage",
      "activeTab"
    ],
    "content_scripts": [
      {
        "css": ["./styles/content.css"],
        "js": ["./scripts/content.js"],
        "matches": ["*://*.edu.ua/mod/quiz/*","*://*.edu.ua/moodle/mod/quiz/*"],
        "run_at": "document_idle"
      }
    ]
  }
  